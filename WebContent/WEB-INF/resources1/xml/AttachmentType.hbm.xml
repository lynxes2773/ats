<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ats.entity">
	<class name="AttachmentType" table="attachments_type_master" mutable="false">
		<id name="id" column="attachment_type_id">
			<generator class="increment"/>
		</id>
		<property name="attachmentTypeName" column="attachment_type_name" not-null="true"/>
	</class>
</hibernate-mapping>