<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ats.entity">
	<class name="PositionType" table="position_type_master" mutable="false">
		<id name="id" column="position_type_id">
			<generator class="increment"/>
		</id>
		<property name="positionTypeDescription" column="position_type_description" not-null="true"/>
	</class>	
</hibernate-mapping>